<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="UsageNotification" name="Usage Notification">
        <div class="o_usage_notification alert alert-info" role="alert">
            <button type="button" class="o_usage_notification_close close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&#215;</span>
            </button>
            <strong><t t-esc="message"/></strong>
        </div>
    </template>
</odoo> 